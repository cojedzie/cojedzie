{
  "name": "kadet/cojedzie",
  "description": "Co Jedzie",
  "type": "project",
  "version": "2023.1.4",
  "license": "AGPL-3.0-only",
  "require": {
    "php": "^8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "baldinof/roadrunner-bundle": "^2.1",
    "cerbero/json-objects": "^1.1",
    "composer/package-versions-deprecated": "1.11.99.4",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2",
    "illuminate/collections": "^8.83",
    "kadet/functional": "dev-master",
    "nelmio/api-doc-bundle": "^4.2",
    "nelmio/cors-bundle": "^2.1",
    "nesbot/carbon": "^2.57.0",
    "nyholm/psr7": "^1.5",
    "ocramius/proxy-manager": "^2.0",
    "php-ds/php-ds": "^1.4",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.2",
    "sensio/framework-extra-bundle": "^6.2",
    "sentry/sentry-symfony": "^4.8",
    "spiral/roadrunner": "^2.8",
    "symfony/amqp-messenger": "6.4.*",
    "symfony/asset": "^6.4",
    "symfony/console": "^6.4",
    "symfony/doctrine-messenger": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/flex": "^2.4",
    "symfony/form": "6.4.*",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-client": "6.4.*",
    "symfony/mercure-bundle": "^0.3.2",
    "symfony/messenger": "6.4.*",
    "symfony/monolog-bundle": "^3.3",
    "symfony/property-access": "6.4.*",
    "symfony/property-info": "6.4.*",
    "symfony/redis-messenger": "6.4.*",
    "symfony/runtime": "6.4.*",
    "symfony/serializer": "6.4.*",
    "symfony/twig-bundle": "^6.4",
    "symfony/uid": "6.4.*",
    "symfony/validator": "6.4.*",
    "symfony/yaml": "^6.4",
    "zenstruck/schedule-bundle": "^1.2"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "secure-http": false,
    "platform": {
      "php": "8.1.1"
    },
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true,
      "php-http/discovery": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "./src/Functions/index.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.4.*"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://git.kadet.net/kadet/functional-php.git"
    }
  ],
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "phpbench/phpbench": "^1.2",
    "phpstan/phpstan": "^1.4",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^1.0.0",
    "symfony/browser-kit": "6.4.*",
    "symfony/css-selector": "6.4.*",
    "symfony/maker-bundle": "^1.30",
    "symfony/phpunit-bridge": "^6.4",
    "symfony/stopwatch": "6.4.*",
    "symfony/web-profiler-bundle": "6.4.*",
    "symplify/easy-coding-standard": "^12.1"
  }
}
